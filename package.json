    "css-loader": "^6.8.1",
    "eslint": "^8.35.0",
    "eslint-plugin-mocha": "^10.1.0",
    "file-loader": "^6.2.0",
    "mocha": "^9.1.0",
    "puppeteer": "^19.7.2",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-ext-reloader": "^1.1.9",
    "webpack-node-externals": "^3.0.0"
